<% include ../partials/header %>

<div id="container" class="container pd-tb-50">
    <% for(var i=0; i < results.length; i++) { %>
      <div class="flex mb-50 container-item content">
          <a href="/topics/<%= results[i].id %>" class="mr-15"><img src="<%= results[i].imageurl %>" alt="" class="width-60 height-60 border-radius" /></a>
          <a class="bold" href="/topics/<%= results[i].id %>"><%= results[i].name %></a>
      </div>
    <% } %>
</div>
<div id="loadMore">
</div>
<script src="/javascripts/loadmoretopics.js"></script>
<% include ../partials/footer %>



